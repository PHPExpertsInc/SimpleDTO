{
    "name": "phpexperts/simple-dto",
    "description": "A quick and easy DTO package.",
    "keywords": [
        "dto",
        "data transfer objects",
        "restful",
        "api"
    ],
    "homepage": "https://www.phpexperts.pro/",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Experts, Inc.",
            "homepage": "https://www.phpexperts.pro/"
        },
        {
            "name": "Theodore R. Smith",
            "homepage": "https://www.linkedin.com/in/tedrsmith"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "nesbot/carbon": "1.*|2.*",
        "phpexperts/datatype-validator": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*|9.*",
        "phpstan/phpstan": "*",
        "symfony/var-dumper": "*",
        "povils/phpmnd": "*",
        "squizlabs/php_codesniffer": "*",
        "phpexperts/dockerize-php8": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PHPExperts\\SimpleDTO\\" : "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "PHPExperts\\SimpleDTO\\Tests\\" : "tests/" }
    }
}
